<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ApplicationIcon />
    <OutputType>Library</OutputType>
    <StartupObject />
    <Version>0.9.7-beta</Version>
    <Authors>Vitae</Authors>
    <PackageProjectUrl>https://github.com/CreatioVitae/WebApiPack</PackageProjectUrl>
    <Description>For ASP.NET Core REST API Pack;

Include Default Builder;</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <FileVersion>0.9.7.0</FileVersion>
    <PackageReleaseNotes>Upgrade WebPack.CoreLib...</PackageReleaseNotes>
    <AssemblyVersion>0.9.7.0</AssemblyVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>none</DebugType>
    <DebugSymbols>false</DebugSymbols>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="WebPack.CoreLib" Version="0.9.1-beta" />
    <PackageReference Include="Utf8Json" Version="1.3.7" />
    <PackageReference Include="Utf8Json.AspNetCoreMvcFormatter" Version="1.3.7" />
  </ItemGroup>
</Project>